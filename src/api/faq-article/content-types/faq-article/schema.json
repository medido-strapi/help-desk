{
  "kind": "collectionType",
  "collectionName": "faq_articles",
  "info": {
    "singularName": "faq-article",
    "pluralName": "faq-articles",
    "displayName": "FAQ Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "unique": true
    },
    "isPopular": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": false
    },
    "distributorContents": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "distributor.faq-article-content",
      "repeatable": true,
      "required": true
    }
  }
}
